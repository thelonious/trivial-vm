; An example of each of the addressing modes

:address
	SET	$0001,				#0
	SET	$0001 + r2,			#0
	SET	[$0001],			#0
	SET	[$0001 + r2],		#0
	SET	[$0001] + r2,		#0
	SET	[$0001 + r2] + r3,	#0

:register
	SET	r2,					#0
	SET	r2 + r3,			#0
	SET	[r2],				#0
	SET	[r2 + r3],			#0
	SET	[r2] + r3,			#0
	SET	[r2 + r3] + r4,		#0

:data
	SET	data,				#0
	SET	data + r3,			#0
	SET	[data],				#0
	SET	[data + r3],		#0
	SET	[data] + r3,		#0
	SET	[data + r3] + r4,	#0